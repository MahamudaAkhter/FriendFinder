<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="6dp"
        android:right="6dp"
        android:bottom="6dp"
        android:left="6dp">
        <shape
            android:shape="oval">
            <solid android:color="#fff" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <size
                android:width="64dp"
                android:height="64dp"/>
            <stroke android:width="2dp"
                android:color="@color/primary"/>
        </shape>
    </item>
</layer-list>